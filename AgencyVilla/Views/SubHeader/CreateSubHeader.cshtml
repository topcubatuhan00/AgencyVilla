@model CreateSubHeaderDto
@{
    ViewData["Title"] = "Üst Bilgi Oluştur";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<form method="post">
    <label>Adres</label>
    <input type="text" placeholder="Adres" asp-for="Address" class="form-control" />
    <br />

    <label>Email</label>
    <input type="text" placeholder="Email" asp-for="Email" class="form-control" />
    <br />

    <label>Facebook</label>
    <input type="text" placeholder="Facebook" asp-for="Facebook" class="form-control" />
    <br />

    <label>Instagram</label>
    <input type="text" placeholder="Instagram" asp-for="Instagram" class="form-control" />
    <br />

    <label>Twitter</label>
    <input type="text" placeholder="Twitter" asp-for="Twitter" class="form-control" />
    <br />

    <label>Linkedin</label>
    <input type="text" placeholder="Linkedin" asp-for="Linkedin" class="form-control" />
    <br />
    <button class="btn btn-outline-success">Kaydet</button>
</form>

